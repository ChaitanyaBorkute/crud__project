<h1>Student Record</h1>

<table class="table">
  <thead>
    <tr>
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>
      <th scope="col">Address</th>
    
    </tr>
  </thead>
  <tbody>

    <tr scope="row">
      <td><%=@student.fname%></td>
      <td><%=@student.lname%></td>
      <td><%=@student.address%></td>
    </tr>
  </tbody>
</table>